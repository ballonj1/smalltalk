<h1>Links#index</h1>
<p>Find me in app/views/links/index.html.erb</p>

<%= link_to "New Link", new_link_url %>

<% @links.each do |link| %>
<%= link_to link.title, link_url(link) %>
<%= link_to "Edit Link", edit_link_url(link) %>
<% end %>

<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<%= error %>
<% end %>
<% end %>
